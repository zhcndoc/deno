  <span class="token comment">// if concurrent execution is required.</span>
  console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span>event<span class="token punctuation">)</span><span class="token punctuation">;</span> <span class="token comment">// prints ['bar'] [42]</span>
