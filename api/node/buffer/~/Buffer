<span class="token comment">// Prints: 6</span>
</code></pre>

      <button id="copy-button-20" class="copyButton" data-copy="import { Buffer } from &apos;node:buffer&apos;;

const buf = Buffer.from(&apos;this is a buffer&apos;);

console.log(buf.indexOf(&apos;this&apos;));
// Prints: 0
console.log(buf.indexOf(&apos;is&apos;));
// Prints: 2
console.log(buf.indexOf(Buffer.from(&apos;a buffer&apos;)));
// Prints: 8
console.log(buf.indexOf(97));
// Prints: 8 (97 is the decimal ASCII value for &apos;a&apos;)
console.log(buf.indexOf(Buffer.from(&apos;a buffer example&apos;)));
// Prints: -1
console.log(buf.indexOf(Buffer.from(&apos;a buffer example&apos;).slice(0, 8)));
// Prints: 8

const utf16Buffer = Buffer.from(&apos;\u039a\u0391\u03a3\u03a3\u0395&apos;, &apos;utf16le&apos;);

console.log(utf16Buffer.indexOf(&apos;\u03a3&apos;, 0, &apos;utf16le&apos;));
// Prints: 4
console.log(utf16Buffer.indexOf(&apos;\u03a3&apos;, -4, &apos;utf16le&apos;));
// Prints: 6
