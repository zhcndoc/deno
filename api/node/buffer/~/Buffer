// Create a `Buffer` and copy data from one region to an overlapping region
// within the same `Buffer`.

const buf = Buffer.allocUnsafe(26);

for (let i = 0; i < 26; i++) {
  // 97 is the decimal ASCII value for &apos;a&apos;.
  buf[i] = i + 97;
}

buf.copy(buf, 0, 4, 10);

